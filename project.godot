; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Astral Threads"
config/description="A 3D, turnbased RPG inspired by the likes of the latter Wizardry & Might & Magic games."
run/main_scene="res://Scenes/Main Menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://Imported Assets/Icon/cyborg-face.svg"

[autoload]

SceneManager="*res://addons/scene_manager/SceneManager.tscn"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
PlayerPartyController="*res://Scripts/Autoloads/PlayerPartyController.gd"
Eventbus="*res://Scripts/Autoloads/Eventbus.gd"
Database="*res://Scripts/Autoloads/Database.gd"
Globals="*res://Scripts/Autoloads/Globals.gd"
PlayerHUD="*res://Scenes/UI/Player Hud.tscn"
BattleHUD="*res://Scenes/UI/Battle Screen/Battle Hud.tscn"
Battlecontroller="*res://Scenes/Combat/Battle Controller.tscn"
PlayerInventory="*res://Scripts/Autoloads/PlayerInventory.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
NumberPopupHandler="*res://Scenes/UI/Battle Screen/Number Popup Handler.tscn"
NpcInteractionListener="*res://Scripts/Autoloads/NPCInteractionListener.gd"

[dialogic]

directories/dch_directory={}
directories/dtl_directory={}
layout/style_list=["res://Game Data/Dialogic Data/Dialogic Style.tres"]
layout/default_style="Default"
glossary/default_case_sensitive=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/scene_manager/plugin.cfg", "res://addons/sound_manager/plugin.cfg", "res://addons/utility_ai/plugin.cfg")

